@model IEnumerable<WCDataConnector.Models.Top10Destination>

@{
    ViewData["Title"] = "Top10Destination";
}
<script src="https://connectors.tableau.com/libs/tableauwdc-2.1.1.js" type="text/javascript"></script>
var myConnector = tableau.makeConnector();
<script type="text/javascript">
    $(document).ready(function () {
        // Create the connector object
        var myConnector = tableau.makeConnector();

        // Define the schema
    myConnector.getSchema = function(schemaCallback) {
        var cols = [{
            id: "Destination",
            dataType: tableau.dataTypeEnum.string
        }, {
            id: "Count",
            dataType: tableau.dataTypeEnum.string
        }];

        var tableSchema = {
            id: "top10Destination",
            alias: "Top 10 Destination a day",
            columns: cols
        };

        schemaCallback([tableSchema]);
        };

    

    });

</script>


<h1>Top10Destination</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>Destination</th>
            <th>Count</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Destination

                <td>


                <td>
                    @item.Count
                    <!--
                    @Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ }) |
                    @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                    @Html.ActionLink("Delete", "Delete", new { /* id=item.PrimaryKey */ })
                        -->
                </td>
            </tr>
        }
    </tbody>
</table>
